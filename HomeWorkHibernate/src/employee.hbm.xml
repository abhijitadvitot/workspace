<!DOCTYPE hibernate-mapping PUBLIC        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>


    <class name="Employee" table="employee" lazy="false">
        <id name="empId" column="Employee_Id">
            <generator class="native"/>
        </id>
        <property name="empName" column="NAME"> </property>


        <property name="mobileNo" column="MobileNO" length="20"> </property>

        <property name="empSalary" column="Salary" length="20"> </property>
        <property name="empDesignation" column="profile" length="20"> </property>

    </class>
</hibernate-mapping>